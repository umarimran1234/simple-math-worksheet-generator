{
	"name": "simple-math-worksheet-generator",
	"version": "2.2.0",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "pnpm run test:integration && pnpm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"prepare": "pnpm smui-theme-light && pnpm smui-theme-dark",
		"smui-theme-light": "smui-theme compile static/smui.min.css -i src/theme -i ./node_modules",
		"smui-theme-dark": "smui-theme compile static/smui-dark.min.css -i src/theme/dark -i ./node_modules",
		"prepare-dev": "pnpm smui-theme-light-expanded && pnpm smui-theme-dark-expanded",
		"smui-theme-light-expanded": "smui-theme compile static/smui.css -i src/theme -i ./node_modules -s expanded",
		"smui-theme-dark-expanded": "smui-theme compile static/smui-dark.css -i src/theme/dark -i ./node_modules -s expanded"
	},
	"devDependencies": {
		"@material/animation": "^14.0.0",
		"@material/base": "^14.0.0",
		"@material/button": "^14.0.0",
		"@material/checkbox": "^14.0.0",
		"@material/chips": "^14.0.0",
		"@material/density": "^14.0.0",
		"@material/dialog": "^14.0.0",
		"@material/dom": "^14.0.0",
		"@material/drawer": "^14.0.0",
		"@material/elevation": "^14.0.0",
		"@material/fab": "^14.0.0",
		"@material/floating-label": "^14.0.0",
		"@material/focus-ring": "^14.0.0",
		"@material/form-field": "^14.0.0",
		"@material/icon-button": "^14.0.0",
		"@material/line-ripple": "^14.0.0",
		"@material/list": "^14.0.0",
		"@material/menu": "^14.0.0",
		"@material/menu-surface": "^14.0.0",
		"@material/notched-outline": "^14.0.0",
		"@material/ripple": "^14.0.0",
		"@material/rtl": "^14.0.0",
		"@material/select": "^14.0.0",
		"@material/shape": "^14.0.0",
		"@material/switch": "^14.0.0",
		"@material/textfield": "^14.0.0",
		"@material/theme": "^14.0.0",
		"@material/tokens": "^14.0.0",
		"@material/tooltip": "^14.0.0",
		"@material/top-app-bar": "^14.0.0",
		"@material/touch-target": "^14.0.0",
		"@material/typography": "^14.0.0",
		"@playwright/test": "^1.37.1",
		"@smui-extra/accordion": "7.0.0-beta.14",
		"@smui/button": "7.0.0-beta.14",
		"@smui/checkbox": "7.0.0-beta.14",
		"@smui/chips": "7.0.0-beta.14",
		"@smui/common": "7.0.0-beta.14",
		"@smui/drawer": "7.0.0-beta.14",
		"@smui/fab": "7.0.0-beta.14",
		"@smui/form-field": "7.0.0-beta.14",
		"@smui/icon-button": "7.0.0-beta.14",
		"@smui/list": "7.0.0-beta.14",
		"@smui/menu": "7.0.0-beta.15",
		"@smui/menu-surface": "7.0.0-beta.15",
		"@smui/paper": "7.0.0-beta.15",
		"@smui/select": "7.0.0-beta.15",
		"@smui/switch": "7.0.0-beta.14",
		"@smui/textfield": "7.0.0-beta.14",
		"@smui/tooltip": "7.0.0-beta.15",
		"@smui/top-app-bar": "7.0.0-beta.14",
		"@smui/touch-target": "7.0.0-beta.15",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/adapter-static": "^2.0.2",
		"@sveltejs/kit": "^1.30.4",
		"@typescript-eslint/eslint-plugin": "^5.62.0",
		"@typescript-eslint/parser": "^5.62.0",
		"autoprefixer": "^10.4.20",
		"eslint": "^8.48.0",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-svelte": "^2.33.0",
